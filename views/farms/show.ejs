<% layout('layouts/boilerplate') %> 
<h1>
    <%= farm.name %>
</h1>
<ul>
    <li>City: <%= farm.city %> 
    </li>
    <li>Email: <%= farm.email %> </li>
</ul>
<a class="btn btn-warning text-white" href="/farms/<%= farm._id %>/edit">Edit Farm</a>
<form class="d-inline" action="/farms/<%= farm._id %>?_method=DELETE" method="POST">
    <button class="btn btn-danger" type="submit">Delete</button>
</form>
<div class="mt-3">
    <a href="/farms">Back</a>
</div>